{
    "name": "codegreencreative/laravel-heroku-deploy",
    "description": "Set up post deploy and pr pre destroy scripts for your Heroku review apps.",
    "keywords": ["laravel", "heroku", "postdeploy", "predestroy", "review apps"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jesse Vista",
            "email": "jesse@codegreencreative.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^7",
        "guzzlehttp/guzzle": "^6.3|^6.4|^6.5|^7.0",
        "illuminate/routing": "^7"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "CodeGreenCreative\\LaravelHerokuDeploy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeGreenCreative\\LaravelHerokuDeploy\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeGreenCreative\\LaravelHerokuDeploy\\LaravelHerokuDeployServiceProvider"
            ]
        }
    }
}
